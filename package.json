{
  "name": "morphonent-test",
  "version": "0.3.0",
  "description": "testkit for morphonent applications",
  "main": "dist/index.js",
  "keywords": [
    "frontend",
    "front end",
    "ui",
    "renderer",
    "functional",
    "testkit",
    "morphonent"
  ],
  "scripts": {
    "test": "jest --coverage --watchAll",
    "test:once": "jest",
    "test:ci": "jest --coverage",
    "build": "rollup -c",
    "lint": "eslint -c .eslintrc.json lib/ test/",
    "lint:fix": "eslint -c .eslintrc.json --fix lib/ test/",
    "verify": "yarn lint && yarn test:ci && yarn build",
    "verify:once": "yarn lint && yarn test:once && yarn build",
    "release:patch": "yarn verify:once && yarn version --patch",
    "release:minor": "yarn verify:once && yarn version --minor",
    "release:major": "yarn verify:once && yarn version --major"
  },
  "repository": "git@github.com:kmruiz/morphonent-test.git",
  "author": "Kevin Mas Ruiz <masruizkevin@gmail.com>",
  "contributors": [
    "Kevin Mas Ruiz <masruizkevin@gmail.com>"
  ],
  "license": "MIT",
  "peerDependencies": {
    "morphonent": "^0.12.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.5.0",
    "coveralls": "^3.0.4",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-utils": "^1.4.2",
    "faker": "^4.1.0",
    "jest": "^24.8.0",
    "morphonent": "^0.12.6",
    "regenerator-runtime": "^0.13.2",
    "rollup": "^1.16.6",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.0"
  }
}
